{
  "name": "MERN_StarterTemplate",
  "version": "2.0.0",
  "description": "FullStack JavaScript ReactJS Express+MongoDB Template",
  "main": "./app.js",
  "scripts": {
    "clientTest": "npm run test --prefix client",
    "server": "npx nodemon ./app.js",
    "client": "npm run start --prefix client",
    "dev": "concurrently \" npm run server\" \" npm run client \" ",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client",
    "start": "node ./app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MERN_StarterTemplate/MERN_StarterTemplate.com.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MERN_StarterTemplate/MERN_StarterTemplate.com/issues"
  },
  "homepage": "https://github.com/MERN_StarterTemplate/MERN_StarterTemplate.com#readme",
  "dependencies": {
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.3",
    "path": "^0.12.7"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
